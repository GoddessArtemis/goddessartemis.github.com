<?xml version="1.0" encoding="utf-8"?>
<rss version="2.0">
 <channel>
  <title>Drama Mama</title>
  <link>{{ site.url }}/</link>
  <description>The scattered thoughts of a new mom.</description>
  <language>en-us</language>
  <copyright>Katie Gietzen</copyright>
  <lastBuildDate>{{ site.time | date: "%a, %d %b %Y %H:%M:%S %z" }}</lastBuildDate>
  <managingEditor>"Katie Gietzen" &lt;goddessartemis85@gmail.com&gt;</managingEditor>
  <webMaster>"Katie Gietzen" &lt;goddessartemis85@gmail.com&gt;</webMaster>
{% for post in site.posts %}
  <item>
    <title>{{ post.title }}</title>
    <link>{{ site.url }}{{ post.url }}</link>
    <pubDate>{{ post.date | date: "%a, %d %b %Y %H:%M:%S %z" }}</pubDate>
    <guid>{{ site.url }}{{ post.id }}</guid>
    <body>
{{ post.content }}
    </body>
  </item>
{% endfor %}
 </channel>
</rss>